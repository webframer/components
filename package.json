{
  "name": "@webframer/ui",
  "version": "0.13.0",
  "description": "Headless UI Components Library",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/webframer/ui",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/webframer/ui.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "ui",
    "components",
    "headless",
    "webframe",
    "react"
  ],
  "scripts": {
    "build": "rm -fr dist/* && tsc -p ../../scripts/npm/@ui-tsconfig.json && node ../../scripts/npm/build.js",
    "push": "npm run build && cd dist && npm publish --access public && cd ../"
  },
  "dependencies": {
    "@webframer/js": "workspace:*",
    "classnames": "^2.3.1",
    "fuse.js": "^6.6.2",
    "prop-types": "^15.8.1",
    "react-json-tree": "^0.17.0",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": "*"
  }
}
